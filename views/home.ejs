<section  id="promo">
<div class="container-fluid"  class="panel">
<div id="myCarousel" class="carousel slide" data-bs-ride="carousel" data-bs-interval="4000">
  <!-- Indicators -->


  <!-- Slides -->
  <div class="carousel-inner ms-4">
    <div class="carousel-item active">
      <img src="images/promo1.png"  style="border-radius: 10px;" alt="Image 1">
    </div>
    <div class="carousel-item">
      <img src="images/promo2.png"  style="border-radius: 10px;" alt="Image 2">
    </div>
    <div class="carousel-item">
      <img src="images/promo3.png"  style="border-radius: 10px;" alt="Image 3">
    </div>
    <!-- Add more slides as needed -->
  </div>
 <ol class="carousel-indicators">
    <li data-bs-target="#myCarousel" data-bs-slide-to="0" class="active"></li>
    <li data-bs-target="#myCarousel" data-bs-slide-to="1"></li>
    <li data-bs-target="#myCarousel" data-bs-slide-to="2"></li>
    <!-- Add more indicators as needed -->
  </ol>
  <!-- Controls -->
  <a class="carousel-control-prev" href="#myCarousel" role="button" data-bs-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Previous</span>
  </a>
  <a class="carousel-control-next" href="#myCarousel" role="button" data-bs-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="visually-hidden">Next</span>
  </a>

</div>

</div>

</section>



