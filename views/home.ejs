<section  id="promo">
<div class="container-fluid"  class="panel">
<div id="myCarousel" class="carousel slide" data-bs-ride="carousel" data-bs-interval="4000">
  <!-- Indicators -->


  <!-- Slides -->
  <div class="carousel-inner ms-4">
    <div class="carousel-item active">
      <img src="images/promo1.png"  style="border-radius: 10px;" alt="Image 1">
    </div>
    <div class="carousel-item">
