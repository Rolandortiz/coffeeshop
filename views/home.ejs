<section id="review-section" class="container-fluid">
<h3 class="h1 text-center" style="color:#ffcd93;">Review from our valued customers</h3>
  <button type="button" class="btn btn-primary mx-auto" id="review-btn" data-bs-toggle="modal" data-bs-target="#exampleModal">
    Make a review!
  </button>

  <div class="ms-4 row">
<% var counter = 0 %>
    <% reviews.docs.slice().reverse().forEach(function(review){ %>
 <% if (counter <=4){ %>
    <div class="col-lg-4 col-md-6 col-sm-12 d-flex justify-content-center mt-5 review" id="reviews">
      <div class="task" draggable="true">
        <div class="tags">
          <span class="tag"><%= review.owner.username %></span>
 <% if (review.createdAt) { %>
    <small class="text-muted"><%= review.createdAt.toLocaleDateString() %></small>
  <% } %>
        </div>

        <p class="text-center"><%= review.body %></p>

<div class="stats">
            <form class="like-form"  method="POST">
  <div class="btn-group">
    <% if (currentUser && review.likes.some(function (like) { %>
    <% return like.equals(currentUser._id)})) { %>
    <button class="btn btn-sm add-like" id="addLikeButton" data-review-id="<%= review._id%>">
      <i class="fas fa-thumbs-up"></i> Liked
    </button>
    <span class="like-count ms-3" style="color: black;"><%= review.likes.length %></span>
    <% } else { %>
