<script>
$(document).ready(function() {
  $(document).on('click', '.pagination-link', function(e) {
  e.preventDefault(); // Prevent default anchor tag behavior

  // Extract page and pageSize values from the clicked link's data attributes
  var page = $(this).data('page');
  var pageSize = $(this).data('pageSize');

  // Perform AJAX request
  $.ajax({
    type: 'GET',
    url: '/', // Replace with the URL of your homepage
    data: { page, pageSize },
    success: function(response) {
      // Create a temporary element to extract the reviews section
      var tempElement = $('<div>').html(response);

      // Find the reviews section within the response HTML
      var reviewSection = tempElement.find('#review-section');

      // Update the reviews section on the page
      $('#review-section').html(reviewSection.html());

      // Update the URL using history API
      const url = `?page=${page}&pageSize=${pageSize}`;
      history.pushState(null, '', url);

      // Scroll to the review section
      $('html, body').animate({
        scrollTop: $('#review-section').offset().top
      }, 500);
    },
    error: function(error) {
      // Handle the error
      console.log(error);
    }
  });
  });

});

</script>
