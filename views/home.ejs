<section id="review-section" class="container-fluid">
  <button type="button" class="btn btn-primary mx-auto" data-bs-toggle="modal" data-bs-target="#exampleModal">
    Make a review!
  </button>
  <div class="row">
    <% for(let review of reviews){ %>
    <div class="col-lg-4 col-md-6 col-sm-12 mt-5">
      <div class="task" draggable="true">
        <div class="tags">
          <span class="tag"><%= review.owner.username %></span>
          <p class="starability-result" data-rating="<%= review.rating %>">
            <small>Rated: <%= review.rating %></small>
          </p>
        </div>
        <p><%= review.body %></p>
        <div class="stats">
          <div></div>
          <div class="viewer"></div>
        </div>
      </div>
    </div>
    <% } %>
  </div>
</section>


